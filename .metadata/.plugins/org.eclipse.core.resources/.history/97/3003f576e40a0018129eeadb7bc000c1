package hw2;
import static hw2.CS227Baseball.*;

public class baseballTest {
	public static void main(String[] args)
	{
		CS227Baseball game = new CS227Baseball(3);
		game.advanceRunners(true);
		System.out.println(game.playerOnFirst()); // expected true
		System.out.println(game.toDisplayString());//0 outs
		game.advanceRunners(false);
		System.out.println(game.playerOnFirst()); // false
		System.out.println(game.playerOnSecond()); // true
		System.out.println(game.toDisplayString());

	 }
}
